{
  "name": "daas-monorepo",
  "private": true,
  "scripts": {
    "start:daas": "pnpm start:test --filter daas",
    "start:dfs": "pnpm start:test --filter dfs",
    "dev:daas": "pnpm start:dev --filter daas",
    "dev:dfs": "pnpm start:dev --filter dfs",
    "build:daas": "pnpm build --filter daas",
    "build:dfs": "pnpm build --filter dfs",
    "clean": "rm -rf node_modules && pnpm -r exec -- rm -rf node_modules"
  },
  "dependencies": {
    "element-ui": "2.15.7",
    "lodash": "^4.17.15",
    "vue": "^2.6.14",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "^6.7.2",
    "prettier": "^2.2.1",
    "sass": "^1.34.1",
    "sass-loader": "^10.2.0"
  },
  "pnpm": {
    "overrides": {
      "@formily/core": "^2.0.17",
      "@formily/element": "^2.0.17",
      "@formily/json-schema": "^2.0.17",
      "@formily/path": "^2.0.17",
      "@formily/reactive": "^2.0.17",
      "@formily/reactive-vue": "^2.0.17",
      "@formily/shared": "^2.0.17",
      "@formily/vue": "^2.0.17"
    }
  }
}
